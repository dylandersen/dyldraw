rules_version = '2';
service firebase.storage {
  match /b/{bucket}/o {
    match /{files}/rooms/{room}/{file} {
    	allow get, write: if true;
    }
    match /{files}/shareLinks/{shareLink}/{file} {
    	allow get, write: if true;
    }
    match /users/{userId}/{allPaths=**} {
      allow read, write: if request.auth != null && request.auth.uid == userId;
    }
  }
}
